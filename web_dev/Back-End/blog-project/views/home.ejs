<%- include('partials/header' ); -%>

  <div class="container">
    <h1>Home</h1>
    <p>
      <%= homeStartingContent %>
    </p>

    <% blogs.forEach((blog)=> { %>
      <h1>
        <%= blog.title %>
      </h1>
      <p>
        <%= blog.post.substring(0, 100) + '...' %>
          <a href="/blogs/<%= blog.title %>">Read More</a>
      </p>
      <% }); %>
  </div>

  <%- include('partials/footer'); -%>